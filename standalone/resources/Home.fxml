<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="homePane" prefHeight="660.0" prefWidth="1056.0" stylesheets="@Style.css"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="test.view.HomeController">
    <ButtonBar buttonMinWidth="280.0" layoutX="70.0" prefHeight="73.0" prefWidth="480.0">
        <buttons>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    onAction="#handleView" text="View" textFill="WHITE">
                <font>
                    <Font name="Magneto Bold" size="30.0"/>
                </font>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    onAction="#handleImport" prefHeight="55.0" text="Import" textFill="WHITE">
                <font>
                    <Font name="Magneto Bold" size="30.0"/>
                </font>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Help"
                    textFill="WHITE">
                <font>
                    <Font name="Magneto Bold" size="30.0"/>
                </font>
            </Button>
        </buttons>
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </ButtonBar>
</AnchorPane>
