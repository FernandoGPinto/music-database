<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<AnchorPane fx:id="dialogAP" xmlns="http://javafx.com/javafx"
            stylesheets="@Style.css"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="test.view.EditDialogController">
    <GridPane fx:id="dialogGP" hgap="15" vgap="10">
        <Button prefHeight="30" prefWidth="120" mnemonicParsing="false" onAction="#handleOk" text="OK"
                GridPane.columnIndex="0" GridPane.rowIndex="4"/>
        <Button prefHeight="30" prefWidth="120" mnemonicParsing="false" onAction="#handleCancel" text="Cancel"
                GridPane.columnIndex="1" GridPane.rowIndex="4"/>
    </GridPane>
</AnchorPane>
